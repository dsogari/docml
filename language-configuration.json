{
  "comments": {
    "blockComment": ["[", "]"]
  },
  "brackets": [["[", "]"]],
  "autoClosingPairs": [
    ["{", "}"],
    ["[", "]"],
    ["(", ")"],
    ["\"", "\""],
    ["'", "'"],
    ["`", "`"],
    ["«", "»"]
  ],
  "surroundingPairs": [
    ["{", "}"],
    ["[", "]"],
    ["(", ")"],
    ["\"", "\""],
    ["'", "'"],
    ["`", "`"],
    ["«", "»"]
  ],
  "folding": {
    "markers": { "start": "\\[", "end": "]" }
  },
  "wordPattern": "((?<=\\\\)[\\xab\\xbb[\\]]|[^\\xab\\xbb[\\]\\s])+",
  "indentationRules": {
    "increaseIndentPattern": "\\[[^\\]]*$",
    "decreaseIndentPattern": "][^\\[]*$"
  }
}
