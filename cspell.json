{
  "$schema": "node_modules/@cspell/cspell-types/cspell.schema.json",
  "version": "0.2",
  "language": "en",
  "dictionaries": ["typescript"],
  "ignoreWords": ["Docml", "docml", "Langium", "Gast"],
  "ignorePaths": [
    ".git",
    ".gitignore",
    ".prettierignore",
    ".vscodeignore",
    "*.lock",
    "*.json",
    "*.toml",
    "*.txt",
    "package.json",
    "*.{js,ts}"
  ],
  "useGitignore": true,
  "enableGlobDot": true
}
