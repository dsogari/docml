{
  "$schema": "node_modules/@cspell/cspell-types/cspell.schema.json",
  "version": "0.2",
  "language": "en",
  "dictionaries": ["typescript"],
  "words": ["guillemets", "overlined"],
  "ignoreWords": [
    "Diego",
    "Sogari",
    "Docml",
    "Langium",
    "PDML",
    "publint",
    "dsogari",
    "codingame",
    "vscodetextmate"
  ],
  "ignorePaths": [
    "*.{js,ts,json,jsonc,lock,svg,toml,git,gitignore,npmignore,prettierignore,vscodeignore}"
  ],
  "useGitignore": true,
  "enableGlobDot": true
}
