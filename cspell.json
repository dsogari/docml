{
  "$schema": "node_modules/@cspell/cspell-types/cspell.schema.json",
  "version": "0.2",
  "language": "en",
  "dictionaries": ["typescript"],
  "words": ["guillemets"],
  "ignoreWords": [
    "Diego",
    "Sogari",
    "Docml",
    "docml",
    "Langium",
    "Gast",
    "publint",
    "vscodeignore"
  ],
  "ignorePaths": [
    ".git",
    ".gitignore",
    ".prettierignore",
    ".vscodeignore",
    "*.lock",
    "*.svg",
    "*.toml",
    "*.txt",
    "package.json",
    "*.{js,ts}"
  ],
  "useGitignore": true,
  "enableGlobDot": true
}
